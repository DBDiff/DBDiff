{
    "name": "dbdiff/dbdiff",
    "type": "project",
    "license": "MIT",
    "bin": ["./dbdiff"],
    "autoload": {
        "psr-4": {
            "DBDiff\\": "src/"
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-pcntl": "*",
        "aura/cli": "2.*@dev",
        "symfony/yaml": "2.8.*@dev",
        "illuminate/database": "4.*",
        "illuminate/view": "4.*",
        "illuminate/container": "4.*",
        "diff/diff": "3.2.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*@dev"
    },
    "config": {
      "platform-check": false
    },
    "scripts": {
      "post-install-cmd": "scripts/post-install.sh",
      "post-update-cmd": "scripts/post-install.sh"
    }
}
